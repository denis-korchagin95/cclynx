@test("It should generate target arm64 on return trivial expression with nested sub-expressions")
@given("file")
int main() {
    return 2 * ((3 * 4 + 20) + 1) * 15;
}
@whenRun("./bin/target-code-generator-tester")
@expectOutput("stdout")
.text
.global _main
.align 2

_main:
    stp x29, x30, [sp, -16]!
    mov x29, sp
    mov x9, #2
    mov x10, #3
    mov x11, #4
    mul x12, x10, x11
    mov x10, #20
    add x11, x12, x10
    mov x10, #1
    add x12, x11, x10
    mul x10, x9, x12
    mov x9, #15
    mul x11, x10, x9
    mov x0, x11
    add sp, sp, #16
    ldp x29, x30, [sp], #16
    ret

@endtest
