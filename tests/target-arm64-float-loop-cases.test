@test("It should generate target arm64 code for while loop with float condition")
@given("file")
int main() {
    float n;
    n = 10.0;
    while (n > 5.0) {
        n = n - 1.0;
    }
    return (int)n;
}
@whenRun("./bin/target-code-generator-tester")
@expectOutput("stdout")
.text
.global _main
.align 2

_main:
    stp x29, x30, [sp, -16]!
    mov x29, sp
    sub sp, sp, #16
    fmov s8, #10.000000
    str s8, [sp, #0]
.L1:
    ldr s8, [sp, #0]
    fmov s9, #5.000000
    cmp s8, s9
    b.le .L2
    ldr s8, [sp, #0]
    fmov s9, #1.000000
    fsub s10, s8, s9
    str s10, [sp, #0]
    b .L1
.L2:
    ldr s8, [sp, #0]
    fcvtzs w9, s8
    mov w0, w9
    add sp, sp, #16
    ldp x29, x30, [sp], #16
    ret

@endtest
